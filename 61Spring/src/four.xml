<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd" >
<beans>
	<bean id="room" class="com.wwq.spring.four.ClassRoom">
		<property name="roomId" value="1"></property>
		<property name="list">
			<list>
				<value>张无忌</value>
				<value>周芷若</value>
				<value>赵敏</value>
			</list>
		</property>
	</bean>
	<bean id="dog" class="com.wwq.spring.four.Dog">
		<property name="age" value="11"></property>
		<property name="set">
			<set>
				<value>骨头</value>
				<value>大骨头</value>
				<value>小骨头</value>
			</set>
		</property>
	</bean>
</beans>
